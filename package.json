        "jquery": "^3.4.1",
        "laravel-mix": "^6.0.43",
        "lodash": "^4.17.15",
        "resolve-url-loader": "^3.1.1",
        "sass": "^1.26.1",
        "sass-loader": "^7.3.1",
        "vue": "^2.6.11",
        "vue-loader": "^15.9.8",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "es6-promise": "^4.2.8",
